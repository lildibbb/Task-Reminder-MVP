{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch --port 3001",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:ds": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/database/data-source.ts migration:run",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "seeds": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli -d src/database/data-source.ts seed",
    "migration:create": "pnpm run typeorm migration:create",
    "migration:generate": "pnpm run typeorm:ds -- migration:generate src/database/migrations/.",
    "migration:run": "pnpm run typeorm:ds -- migration:run",
    "migration:revert": "pnpm run typeorm:ds -- migration:revert",
    "keys": "ts-node src/subscriptions/helpers/generateVapidKeys.ts"
  },
  "lint-staged": {
    "**/*ts": [
      "npm run lint"
    ],
    "**/*.{js,jsx,json}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@keyv/redis": "^4.3.3",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^5.0.0",
    "@types/decimal.js": "^7.4.3",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.7",
    "enhanced-ms": "^4.1.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "ioredis": "^5.6.0",
    "keyv": "^5.3.2",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "mime-types": "^3.0.1",
    "multer": "1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "pluralize": "^8.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.22",
    "typeorm-extension": "^3.7.0",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
